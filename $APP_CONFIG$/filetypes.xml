<application>
  <component name="CommunityFileTypes" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;*.orig;" />
    <extensionMap>
      <mapping ext="tokens" type="PLAIN_TEXT" />
      <mapping ext="tagx" type="XML" />
      <mapping ext="jspx" type="XML" />
    </extensionMap>
    <setting name="fileTypeChangedCounter" value="2" />
  </component>
</application>